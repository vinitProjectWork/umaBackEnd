{
  "kind": "collectionType",
  "collectionName": "sub_categories",
  "info": {
    "singularName": "sub-category",
    "pluralName": "sub-categories",
    "displayName": "Sub Category"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "order": {
      "type": "integer"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "sub_categories"
    }
  }
}
